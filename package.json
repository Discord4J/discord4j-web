{
  "name": "discord4j-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "yarn types && yarn generate && vue-cli-service serve --open",
    "build": "yarn types && yarn generate && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "types": "cp @types/* ./node_modules/@types",
    "clean": "rimraf prerender-routes.js src/autoblog/",
    "generate": "yarn run vue-autoblog"
  },
  "dependencies": {
    "puppeteer": "^1.7.0",
    "vue": "^2.5.17",
    "vue-class-component": "^6.2.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/prismjs": "^1.9.0",
    "@vue/cli": "^3.0.0",
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-typescript": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "bulma": "^0.7.1",
    "dts-gen": "^0.5.7",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.3",
    "prerender-spa-plugin": "^3.2.1",
    "prettier": "^1.14.2",
    "prismjs": "^1.15.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.0.1",
    "vue-autoblog": "0.2.1",
    "vue-meta": "^1.5.2",
    "vue-prism-component": "^1.0.1",
    "vue-scrollto": "^2.11.0",
    "vue-template-compiler": "^2.5.16"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
